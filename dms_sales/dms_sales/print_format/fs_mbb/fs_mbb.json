 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Invoice</title>\n<style>\n    body {\n        font-family: Arial, sans-serif;\n        margin: 0;\n        padding: 0;\n        display: flex;\n        flex-direction: column;\n        min-height: 100vh;\n    }\n\n    .container {\n        max-width: 800px;\n        margin: 0 auto;\n        padding: 20px;\n        flex: 1; /* This makes the container grow to fill the available space */\n    }\n\n    .header {\n        text-align: left;\n        margin-bottom: 20px;\n    }\n\n    .logo-container {\n        display: flex;\n        justify-content: space-between;\n    }\n\n    .logo {\n        max-width: 200px;\n    }\n\n    .address {\n        font-size: 12px;\n    }\n\n    .letter {\n        text-align: center;\n        margin-top: 20px;\n    }\n    \n    .line, .line2 {\n        margin-bottom: 2px;\n        text-align: left;\n        font-size: 12px;\n    }\n\n    .labels {\n        display: inline-block;\n        width: 100px;\n    }\n\n    .label1 {\n        width: 100%;\n    }\n\n    .colon {\n        margin-right: 40px;\n    }\n\n    .colon2 {\n        margin-right: 43px;\n    }\n\n    .colon3 {\n        margin-left: 80px;\n    }\n\n    .colon4, .colon5 {\n        margin-left: 5px;\n    }\n\n    .section p {\n        margin: 0;\n        padding-left: 20px;\n        text-indent: -20px; /* Negative text-indent to pull back the second line */\n    }\n\n    .footer {\n        background-color: #ff8a3a;\n        padding: 5px;\n        text-align: center;\n        border: 1px solid #e8823d;\n        position: absolute;\n        bottom: 0;\n        width: 100%;\n    }\n\n    .footer-text {\n        font-weight: bold;\n        font-size: 10px;\n        color: white;\n        display: inline-block;\n    }\n\n    table {\n        width: 100%;\n        border-collapse: collapse;\n        margin-bottom: 0px;\n    }\n\n    table, th, td {\n        border: 0px solid #000;\n    }\n\n    th, td {\n        padding: 4px;\n        font-size: 12px;\n    }\n</style>\n</head>\n<body>\n    <div class=\"container\">\n        <div class=\"header\">\n            <div class=\"logo-container\">\n                <div>\n                    <p>Authorised Sales & Service Parts Dealer</p>\n                    <img class=\"logo\" src=\"https://dcautoservices.mydotconsulting.com/files/perodua-logo.png\" alt=\"Company Logo 1\" width=\"200\">\n                    <div class=\"address\">\n                    </div>\n                </div>\n                <!--\n                <img class=\"logo\" src=\"\" alt=\"Company Logo 2\" width=\"200\" height=\"45\">\n                -->\n            </div>\n            <div class=\"letter\">\n                <br><br>\n                <div class=\"line\">\n                    <span class=\"labels\"><b>Our Ref No</b></span><span class=\"colon\">:</span>\n                    <span><b></b></span>\n                </div><br>\n                <div class=\"line\">\n                    <span class=\"labels\"><b>Date</b></span><span class=\"colon\">:</span>\n                    <span><b>{{ frappe.utils.formatdate(doc.date, \"dd MMMM yyyy\") }}</b></span>\n                </div><br>\n                <div class=\"line\">\n                    <span class=\"labels\"><b>To</b></span><span class=\"colon\">:</span>\n                    <span><b>MAYBANK AUTO HUB SELAYANG</b></span>\n                </div>\n                <div class=\"line\">\n                    <span class=\"labels\"></span><span class=\"colon2\"></span>\n                    <span><b>NO 167-175 2ND FLOOR JALAN 2/3A</b></span>\n                </div>\n                <div class=\"line\">\n                    <span class=\"labels\"></span><span class=\"colon2\"></span>\n                    <span><b>BANDAR PUSAT UTARA</b></span>\n                </div>\n                <div class=\"line\">\n                    <span class=\"labels\"></span><span class=\"colon2\"></span>\n                    <span><b>68100 BATU CAVES SELANGOR</b></span>\n                </div>\n                <br>\n                <div class=\"line\">\n                    <span class=\"label1\"><b>Attention</b></span>\n                </div>\n                <br>\n                <div class=\"line\">\n                    <span class=\"label1\" style=\"text-decoration: underline\"><b>RE: Request To Utilise Floor Stocking Facility</b></span>\n                </div>\n                <div class=\"line\">\n                    <span class=\"label1\"><b>With reference to the above, we would like to finance the below listed vehicles as allocated to us by Perodua Sales Sdn. Bhd. under the Floor Stocking Facility:</b></span>\n                </div>\n                <br>\n                {% block table_lhti %}\n                <table style=\"border-collapse: collapse;\">\n                    <thead>\n                        <tr>\n                            <th style=\"border: 1px solid #000; padding: 10px; text-align: center; color: black; background: lightgray\"><b>NO</b></th>\n                            <th style=\"border: 1px solid #000; padding: 10px; text-align: center; color: black; background: lightgray\"><b>CHASSIS NO</b></th>\n                            <th style=\"border: 1px solid #000; padding: 10px; text-align: center; color: black; background: lightgray\"><b>ENGINE NO</b></th>\n                            <th style=\"border: 1px solid #000; padding: 10px; text-align: center; color: black; background: lightgray\"><b>MODEL</b></th>\n                            <th style=\"border: 1px solid #000; padding: 10px; text-align: center; color: black; background: lightgray\"><b>AMOUNT (RM)</b></th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {% for row in doc.table_lhti %}\n                        <tr>\n                            <td style=\"border: 1px solid #000; padding: 10px;\"><b>{{ loop.index }}</b></td>\n                            <td style=\"border: 1px solid #000; padding: 10px;\"><b>{{ row.chassis }}</b></td>\n                            <td style=\"border: 1px solid #000; padding: 10px;\"><b>{{ row.engine_number }}</b></td>\n                            <td style=\"border: 1px solid #000; padding: 10px;\"><b>{{ row.model }}</b></td>\n                            <td style=\"border: 1px solid #000; padding: 10px;\"><b>{{ frappe.utils.fmt_money(row.amount, 2) }}</b></td>\n                        </tr>\n                        {% endfor %}\n                    </tbody>\n                </table>\n                {% endblock %}\n                <br>\n                <p style=\"text-align: left;\">\n                    <br><br><br>\n                    Yours faithfully,<br>\n                    <!--<img src=\"\" width=\"80\" height=\"70\">-->\n                    <!--<img src=\"\" width=\"150\" height=\"50\"><br>-->\n                    <!--------------------------------------------<br>-->\n                     <img src=\"https://rohamas.mydotconsulting.com/files/sign%20fairuz.jpg\" width=\"70\" height=\"50\">\n                    <img src=\"https://rohamas.mydotconsulting.com/files/cop%20rohamas.png\" width=\"80\" height=\"80\"><br>\n                     ------------------------------------------------------------- <br>\n                    <b>ROHAMAS PRO AUTOSERV SDN BHD\n                </p>\n            </div>\n        </div>\n    </div>\n    <!--<div class=\"footer\">-->\n    <!--    <div class=\"footer-text\">-->\n    <!--        Rohamas Pro Autoserv Sdn Bhd (786823-T), Lot 91517, Persiaran Atmosfera Seksyen U5, 40150 Shah Alam Selangor <br>Tel: 012-8835282-->\n    <!--    </div>-->\n    <!--</div>-->\n</body>\n</html>\n",
