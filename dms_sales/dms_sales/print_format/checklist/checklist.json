 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n    <style>\r\n        body {\r\n            font-family: Arial, sans-serif;\r\n        }\r\n        .container {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            padding: 10px;\r\n        }\r\n        .left-column {\r\n            display: flex;\r\n            align-items: center;\r\n            width: 70%;\r\n        }\r\n        .company-address {\r\n            margin-left: 20px;\r\n        }\r\n        .right-column {\r\n            flex: 1;\r\n            text-align: right;\r\n        }\r\n        table {\r\n            width: 100%;\r\n            border-collapse: collapse;\r\n            margin-bottom: 10px;\r\n        \r\n        }\r\n        th, td {\r\n            border: 1px solid #000;\r\n            padding: 1px;\r\n            text-align: left;\r\n            font-size: 11px;\r\n           \r\n        }\r\n        .column {\r\n            float: left;\r\n            width: 50%;\r\n            box-sizing: border-box;\r\n        }\r\n        .column2 {\r\n            width: 100%;\r\n            box-sizing: border-box;\r\n            font-size: 10px;\r\n        }\r\n        .row:after {\r\n            content: \"\";\r\n            display: table;\r\n            clear: both;\r\n        }\r\n        hr {\r\n            margin-top: 10px;\r\n            margin-bottom: 0px;\r\n            border: 0;\r\n            border-top: 1px solid #d1d8dd;\r\n        }\r\n        .container2 {\r\n            text-align: center;\r\n            display: flex;\r\n            justify-content: space-between;\r\n            padding: 2px;\r\n        }\r\n        .verified-column {\r\n            flex: 1;\r\n            text-align: left;\r\n            padding: 0px;\r\n        }\r\n        .column3 {\r\n            float: right;\r\n            box-sizing: border-box;\r\n        }\r\n      .fixed-height {\r\n            min-height: 50px; /* Set your desired minimum height */\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <div class=\"container\">\r\n        <!-- Left Column -->\r\n        <div class=\"left-column\">\r\n            <img src=\"https://dcautoservices.mydotconsulting.com/files/Perodua_logo_2008.png\" alt=\"Company Logo\" width=\"100\">\r\n            <div class=\"company-address\">\r\n                <span style=\"font-size:10px\">Lot 91517 Persiaran Atmosfera Seksyen U5</span><br>\r\n                <span style=\"font-size:10px\">40150 Shah Alam Selangor</span><br>\r\n                <span style=\"font-size:10px\">Tel: 012-8835282</span><br>\r\n                <span style=\"font-size:12px\">MONTH: <span style=\"color:blue\"><b>{{ doc.month | upper }}</b></span></span><br>\r\n                <span style=\"font-size:12px\">YEAR: <span style=\"color:blue\"><b>{{ frappe.utils.formatdate(doc.registration_date, \"yyyy\") }}</b></span></span><br>\r\n                <span style=\"font-size:14px\"><b><center>DETAILS & CHECKLIST</center></b></span>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Right Column -->\r\n        <div class=\"right-column\">\r\n            <table>\r\n                <tr><td><b>EHAKMILIK</b></td></tr>\r\n                <tr><td><b>DOWNPAYMENT</b></td></tr>\r\n                <tr><td><b>GEARUP</b></td></tr>\r\n                <tr><td><b>INSURANCE</b></td></tr>\r\n                <tr><td><b>INVOICE</b></td></tr>\r\n                <tr><td><b>XERO</b></td></tr>\r\n                <tr><td><b>CA</b></td></tr>\r\n                <tr><td><b>BUY NO</b></td></tr>\r\n            </table>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Tables to match the provided layout -->\r\n    <div class=\"container2\">\r\n      <div class=\"column\" style=\"width:26%\">\r\n            <table>\r\n                <tr>\r\n                    <td class=\"no-border\">SALES ADVISOR</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">BOOKING DATE</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">MODEL</td>\r\n                  \r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">CHASSIS NO.</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">ENGINE NO.</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">CUSTOMER'S NAME</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">CUSTOMER'S IC NO.</td>\r\n                </tr>\r\n            </table>\r\n      \r\n        </div>\r\n      \r\n            <div class=\"column\" style=\"width:70%\">\r\n            <table>\r\n                <tr>\r\n                  <td colspan=\"2\" style=\"text-align:center\"><b>{{doc.sales_advisor}}</b></td>\r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"2\" style=\"text-align:center\"><b>{{ frappe.utils.formatdate(doc.registration_date, \"dd/mm/yyyy\") }}</b></td>\r\n                </tr>\r\n                <tr>\r\n                     <td style=\"text-align:center; width:50%\" ><b>{{doc.car_model_color}}</b></td>\r\n                      <td style=\"text-align:center;width: 50%\"><b>{{doc.color_choice}}</b></td>\r\n                  \r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"2\" style=\"text-align:center\"><b>{{doc.chassis_number}}</b></td>\r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"2\" style=\"text-align:center\"><b>{{doc.engine_number}}</b></td>\r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"2\" style=\"text-align:center\"><b>{{doc.customer_name}}</b></td>\r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"2\" style=\"width:70%;text-align:center\"><b>{{doc.customer_ic}}</b></td>\r\n                </tr>\r\n            </table>\r\n      \r\n        </div>\r\n            </div>\r\n              \r\n               <div class=\"container2\">\r\n                    \r\n           <div class=\"column\" style=\"width:26%\">\r\n         \t <table>\r\n                <tr>\r\n                    <td class=\"no-border\">BANK (LOU)</td>\r\n                </tr>\r\n            </table>\r\n           </div>\r\n          <div class=\"column\" style=\"width:70%\">\r\n         \t <table>\r\n                <tr>\r\n                  <td style=\"width:70%;text-align:center\"><b>{{doc.finance}}</b>\r\n                </tr>\r\n            </table>\r\n           </div>\r\n          </div>\r\n              \r\n        <div class=\"container2\">\r\n                    \r\n           <div class=\"column\" style=\"width:30%\">\r\n         \t <table>\r\n                <tr>\r\n                    <td style=\"border:none\">ADD. WINDSCREEN INS:</td>\r\n                </tr>\r\n            </table>\r\n           </div>\r\n          <div class=\"column\" style=\"width:23%; margin-right:150px\">\r\n         \t <table>\r\n                <tr>\r\n                  <td >&nbsp;</td>\r\n                </tr>\r\n            </table>\r\n           </div>\r\n          </div>\r\n              \r\n              \r\n\r\n    <div class=\"container2\">\r\n      \r\n      \r\n      \r\n       <div class=\"column\" style=\"width:26%\">\r\n          <table>\r\n               \r\n                <tr>\r\n                    <td class=\"no-border\">ORIGINAL OTR (RM) W/O INS</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">PLATE NUMBER</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">HP</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">REG FEE</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"no-border\">ROAD TAX</td>\